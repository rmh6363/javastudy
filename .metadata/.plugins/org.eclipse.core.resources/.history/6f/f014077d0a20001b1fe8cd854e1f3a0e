package com.daum;

public class Position {
	private String name;
	private int power;
	public Position() {
		// TODO Auto-generated constructor stub
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getPower() {
		return power;
	}
	public void setPower(int power) {
		this.power = power;
	}
	public Position(String name, int power) {
		super();
		this.name = name;
		this.power = power;
	}
	public void attack(Baron nasher) {
		int bhp = nasher.getHp();
		int ahp = bhp - power;Position
		
		System.out.println("¹Ù·ÐÀÇ hp :" + ahp);
	}
	
	

}

