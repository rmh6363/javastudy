delete from member
commit
select * from member
drop table member
create table member(
mid varchar2(10),
name varchar2(10),
job varchar2(10),
birth date
)
alter table member add constraint pk_member_member_num primary key(member_num)

create table rank(
rank_number varchar2(10),
rank_name varchar2(10))
drop table memu
create table menu2(
fullname varchar2(100) primary key
)
insert into menu2 values('com.naver.InsertCommand')
insert into menu2 values('com.naver.SelectCommand')
insert into menu2 values('com.naver.UpdateCommand')
insert into menu2 values('com.naver.DeleteCommand')
insert into menu2 values('com.naver.EndCommand')
