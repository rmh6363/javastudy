package com.naver;

import java.util.Random;

public class Test {
	public void me4(int num) {
		Random rnd = new Random();
		for (int i = 0; i <11 ; i++) {
			int ran = rnd.nextInt(num);
			if (num>2 && num<11) {
				System.out.println(ran);
			}
		}
		
	}
	
	
	
	
	public void me3() {
		Random rnd = new Random();
		for (int i = 0; i < 100; i++) {
			int num = rnd.nextInt(3);
			System.out.println(num);
		}
		
	}
	
	public void me2() {
		Random rnd = new Random(System.currentTimeMillis());
		
	}
	
	
	public void me1() {
		Random rnd = new Random();
		
	}
	
	

}
