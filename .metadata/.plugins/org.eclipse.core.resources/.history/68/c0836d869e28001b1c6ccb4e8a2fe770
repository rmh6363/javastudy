package com.naver;

public class Sum {
	private int num;
	private int tes;
	
	Object key1 = new Object();
	
	public Sum(int num, int tes) {
		super();
		this.num = num;
		this.tes = tes;
	}

	public int getTes() {
		return tes;
	}

	public void setTes(int tes) {
		this.tes = tes;
	}

	public Sum() {
		// TODO Auto-generated constructor stub
	}

	public Sum(int num) {
		super();
		this.num = num;
	}

	public int getNum() {
		return num;
	}

	public void setNum(int num) {
		this.num = num;
	}
	public void subTes(int n) {
		synchronized (key1) {
			tes -= n;
		}
	}
	public void addNum(int n) {
		synchronized (this) {
			//임계영역,critical section
			num += n;
		}
	}

}
