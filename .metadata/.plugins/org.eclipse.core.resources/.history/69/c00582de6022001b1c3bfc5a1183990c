	}

	public Sword(int power, String name) {
		super(power, name);
		// TODO Auto-generated constructor stub
	}

	public Sword(int power) {
		super(power);
		// TODO Auto-generated constructor stub
	}@Override
	public void img() {
		System.out.println("검으로 마구 벱니다.");
	}
	
	
	

}
